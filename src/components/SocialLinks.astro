---
import presentation from "@/data/presentation";
---

<ul role="list" class="flex flex-row gap-2">
  {
    presentation.socials.map((social, index) => (
      <>
        <li>
          <a href={social.link} class="hover:text-neutral-100 cursor-pointer text-3xl" rel="noreferrer" target="_blank">
            <i class={`bi bi-${social.icon}`}></i>
          </a>
        </li>
      </>
    ))
  }
</ul>
